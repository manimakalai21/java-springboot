pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building. is in progress.'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..in progress '
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....new buid'
            }
        }
        stage('success'){
            steps{
                echo 'sucessful'
            }
        }
    }
}